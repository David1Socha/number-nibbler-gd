config_version=4

[application]

config/name="Number Nibbler"
run/main_scene="res://MainMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Number Nibbler"
boot_splash/image="res://number_nibbler_icon_432.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.298039, 0.858824, 1, 1 )
config/icon="res://number_nibbler_icon_1024.png"
config/windows_native_icon="res://number_nibbler_icon.ico"
config/quit_on_go_back=false

[autoload]

Startup="*res://Scripts/Startup.cs"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="expand"

